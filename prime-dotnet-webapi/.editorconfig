# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.ts]
indent_size = 2

[*.cs]
indent_size = 4

[Dockerfile]
indent_size = 4

[*.dockerfile]
indent_size = 4

[*.md]
max_line_length = off
trim_trailing_whitespace = false

[Migrations/*]
generated_code = true

[Configuration/Resources/*.html]
generated_code = true
