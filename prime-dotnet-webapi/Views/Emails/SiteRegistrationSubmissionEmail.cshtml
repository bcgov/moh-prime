@using Prime.Services
@model EmailParams

@{
  var documentUrl = Model.DocumentUrl;
  var expiryDays = Model.ExpiryDays;
}

<p>
  A new PharmaNet site registration has been received. See the attached registration, and organization agreement for more information.
</p>

<p>
  To access the Business Licence, click this link <a href="@documentUrl">@documentUrl</a>.

  @if(expiryDays > 0) {
    <strong>This link is only accessible for {expiryDays} days.</strong>
  }
</p>
