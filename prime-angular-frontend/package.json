{
  "name": "angular-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ie": "ng serve --configuration es5",
    "preci": "rm -fr node_modules",
    "ci": "yarn install --frozen-lockfile",
    "build": "NG_PERSISTENT_BUILD_CACHE=1 ng build --subresource-integrity",
    "test": "ng test --watch=false --code-coverage --browsers=ChromeHeadlessCustom",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "reporter": "ng build --configuration production --stats-json --source-map",
    "bundle-report": "npm run reporter && webpack-bundle-analyzer dist/angular-frontend/stats-es2015.json",
    "source-report": "npm run reporter && source-map-explorer dist/angular-frontend/main-es2015.**.js",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.12",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.3.12",
    "@angular/compiler": "^13.3.12",
    "@angular/core": "^13.3.12",
    "@angular/forms": "^13.3.12",
    "@angular/material": "^13.3.9",
    "@angular/material-moment-adapter": "^13.3.9",
    "@angular/platform-browser": "^13.3.12",
    "@angular/platform-browser-dynamic": "^13.3.12",
    "@angular/router": "^13.3.12",
    "@auth0/angular-jwt": "^5.0.2",
    "@ngneat/until-destroy": "^8.1.4",
    "@tinymce/tinymce-angular": "^4.2.4",
    "bootstrap": "^4.5.3",
    "classlist.js": "^1.1.20150312",
    "filepond": "^4.22.1",
    "filepond-plugin-file-validate-size": "^2.2.1",
    "filepond-plugin-file-validate-type": "^1.2.5",
    "keycloak-angular": "^9.1.0",
    "keycloak-js": "^15.1.1",
    "moment": "^2.29.3",
    "ng-busy": "^13.1.1",
    "ngx-filepond": "^6.0.1",
    "ngx-mask": "^12.0.0",
    "ngx-progressbar": "^6.1.1",
    "popper.js": "^1.16.0",
    "rxjs": "~7.5.4",
    "smoothscroll-polyfill": "^0.4.4",
    "tinymce": "^7.0.0",
    "tslib": "^2.0.0",
    "tus-js-client": "^1.8.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.11",
    "@angular-eslint/builder": "13.3.0",
    "@angular-eslint/eslint-plugin": "13.3.0",
    "@angular-eslint/eslint-plugin-template": "13.3.0",
    "@angular-eslint/schematics": "13.3.0",
    "@angular-eslint/template-parser": "13.3.0",
    "@angular/cli": "^13.3.11",
    "@angular/compiler-cli": "^13.3.12",
    "@angular/language-service": "^13.3.12",
    "@types/faker": "^4.1.12",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/tus-js-client": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "^7.26.0",
    "faker": "^4.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.15",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.4.1",
    "ts-node": "~7.0.0",
    "typescript": "~4.6",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}
