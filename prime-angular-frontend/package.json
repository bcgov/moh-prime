{
  "name": "angular-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ie": "ng serve --configuration es5",
    "build": "ng build --subresource-integrity",
    "audit": "npm install && npm audit fix && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "coverage": "ng test --watch=false --code-coverage --browsers=ChromeHeadlessNoSandbox",
    "sonar": "npm run coverage && sonar-scanner",
    "reporter": "ng build --prod --stats-json --sourceMap",
    "bundle-report": "npm reporter && webpack-bundle-analyzer dist/angular-frontend/stats-es2015.json",
    "source-report": "npm reporter && source-map-explorer dist/angular-frontend/main-es2015.**.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.3",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "^10.2.3",
    "@angular/compiler": "^10.2.3",
    "@angular/core": "^10.2.3",
    "@angular/forms": "^10.2.3",
    "@angular/material": "^9.2.0",
    "@angular/material-moment-adapter": "^9.2.0",
    "@angular/platform-browser": "^10.2.3",
    "@angular/platform-browser-dynamic": "^10.2.3",
    "@angular/router": "^10.2.3",
    "@auth0/angular-jwt": "^3.0.1",
    "@tinymce/tinymce-angular": "^3.3.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/tus-js-client": "^1.8.0",
    "bootstrap": "^4.4.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.6.4",
    "filepond": "^4.22.1",
    "filepond-plugin-file-validate-size": "^2.2.1",
    "filepond-plugin-file-validate-type": "^1.2.5",
    "keycloak-angular": "^7.0.1",
    "keycloak-js": "^6.0.1",
    "moment": "^2.24.0",
    "ng-busy": "^9.0.0",
    "ngx-clipboard": "^12.2.1",
    "ngx-filepond": "^5.0.1",
    "ngx-mask": "^8.1.6",
    "ngx-progressbar": "^6.0.2",
    "popper.js": "^1.16.0",
    "rxjs": "~6.5.4",
    "smoothscroll-polyfill": "^0.4.4",
    "tinymce": "^5.1.6",
    "tslib": "^2.0.0",
    "tus-js-client": "^1.8.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "^10.2.3",
    "@angular/language-service": "^10.2.3",
    "@types/faker": "^4.1.7",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "faker": "^4.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "sonar-scanner": "~3.1.0",
    "source-map-explorer": "^2.4.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}
